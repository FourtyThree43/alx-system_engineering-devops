  global
      log /dev/log local0
      log /dev/log local1 notice
      chroot /var/lib/haproxy
      stats socket /run/haproxy/admin.sock mode 660 level admin
      stats timeout 30s
      user haproxy
      group haproxy
      daemon

  defaults
      log global
      mode http
      option httplog
      option dontlognull
      timeout connect 5000
      timeout client 50000
      timeout server 50000
      timeout http-request 50000

  frontend FT43-tech-frontend
      bind *:80
      default_backend FT43-tech-servers

  backend FT43-tech-servers-backend
      balance roundrobin
      server 195416-web-01 54.160.120.200:80 check
      server 195416-web-02 54.145.155.255:80 check
